@model  API_WEB_DOC.Controllers.util.MODELS.BASE_LOGIN
@{
    ViewBag.Title = "INICIO";
}

<div class="container-fluid">
    <div class="row justify-content-center">
        <div class="col-lg-8">
            <div class="card shadow mb-1  w-100">
                <div class="card-header py-3">
                    <h6 class="m-0 font-weight-bold text-primary">Bienvenido</h6>
                </div>
                <div class="card-body small">
                    <div class="row m-1">
                        <div class="col-lg-4 text-dark text-right">USERNAME</div>
                        <div class="col-lg-8">@Model.USUARIO.UserName.ToUpper()</div>
                    </div>
                    <div class="row m-1">
                        <div class="col-lg-4 text-dark text-right">NOMBRE</div>
                        <div class="col-lg-8">@Model.USUARIO.Nombres.ToUpper()  </div>
                    </div>
                    <div class="row m-1">
                        <div class="col-lg-4 text-dark text-right">APELLIDOS</div>
                        <div class="col-lg-8">@Model.USUARIO.ApellidoPat.ToUpper() @Model.USUARIO.ApellidoMat.ToUpper()</div>
                    </div>
                    <div class="row m-1">
                        <div class="col-lg-4 text-dark text-right">EMPRESA</div>
                        <div class="col-lg-8">@Model.EMPRESA.Rut.ToUpper() @Model.EMPRESA.RazonSocial.ToUpper() </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
